{
    "execMap": {
    },
    "restartable": "rs",
    "ignore": [
        ".vscode",
        "node_modules",
        ".babelrc",
        ".gitignore",
        ".git",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "events": {
        "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
    },
    "watch": [
        "dist/dev/",
        "server/node/"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ejs,html,css,js,json"
}