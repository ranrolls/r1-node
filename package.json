{
  "scripts": {
    "start": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "app-build": "npm run server-build && npm run client-build",
    "server-dev": "nodemon server/node/config/express-server.js --watch ",
    "sb": "npm run server-build",
    "server-build": "node server/node/config/express-server.js",
    "server-prod": "node server/node/config/express-server.js",
    "run-prod": "echo \"Runing production code\"",
    "build": "npm run app-build",
    "cb": "npm run client-build",
    "cd": "npm run client-dev",
    "client-build": "webpack --config common/build-config/wp-config.js --progress -d --env=dev --mode=development --module=login",
    "clinet-prod": "NODE_ENV=production webpack --config common/build-config/prod-config.js -p --env=prod",
    "client-dev": "webpack-serve --config common/build-config/wp-config.js --open",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "tsc:b": "tsc --rootDir src --outDir",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
